programa {
  funcao inicio() {
    inteiro distancia
    real gasolina
    real consumo

  escreva ("digite a distancia percorrida: ")
  leia(distancia)

  escreva("o combustivel consumido: ")
  leia(gasolina)

  consumo = gasolina / distancia

  escreva ("a media de consumo de combustivel é: ", consumo , "L")

  }
}
